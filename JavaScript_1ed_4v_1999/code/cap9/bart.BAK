<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 3.2 Final//EN">

<HTML><HEAD>
	<TITLE>Imagens</TITLE>
	<script>
	
	apagado = new Image();
	aceso   = new Image();
	apagado.src = "Bart-lt.gif";
	aceso.src   = "Bart.gif";
	
	function apaga() {
		document.images[0].src = apagado.src;
	}
	
	function acende() {
		document.images[0].src = aceso.src;
	}
		
	</script>
</HEAD><BODY>
<a href="" onmouseover="acende()" onmouseout="apaga()">
<IMG SRC="Bart-lt.gif" WIDTH=43 HEIGHT=58 BORDER=0></a>
</BODY>
</HTML>
